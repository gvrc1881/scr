	<queryString>
		<![CDATA[select f.created_date,f.remarks,$P{fromDate} as fromDate,
f.max_demand,
f.feed_of,
to_char(f.from_date_time,'hh24:mi') as fromTime,
to_char(f.thru_date_time,'hh24:mi') as thruTime,
f.duration,
f.extended_of,
f.feed_extended_from_date_time,
f.feed_extended_thru_date_time,
f.feed_extended_duration
from failures f
LEFT OUTER JOIN facility FAC ON (f.sub_station = fac.facility_id )
where to_char(created_date, 'yyyy-mm-dd')::date =$P{fromDate} ::date
and f.type_of_failure ='GRID_FAILURE'
and f.current_status = 'ACTIVE']]>
