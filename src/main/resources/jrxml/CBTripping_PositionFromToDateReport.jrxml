	</parameter>
	<parameter name="depot" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[
select $P{fromDate} fromDate,$P{toDate} as toDate,
created_date,
FAC.division,
facility_name,
cascade_assets,
relay_indication ,
f.remarks,load_relief_break_down,
to_char(f.from_date_time, 'HH24:MI')as tripedat,
to_char(f.thru_date_time, 'HH24:MI')as closedat
from failures f
LEFT OUTER JOIN facility FAC ON (f.sub_station = fac.facility_id )
where
to_char(f.from_date_time, 'yyyy-mm-dd')::date >= $P{fromDate}::date
and to_char(f.from_date_time, 'yyyy-mm-dd')::date <= $P{toDate}::date
and case when $P{equipmentno} != '' then cascade_assets = $P{equipmentno}
else cascade_assets is not null or cascade_assets is null end
and case when $P{depot} != '' then facility_name = $P{depot}
else facility_name is not null or facility_name is null end
and type_of_failure ='POWER_FAILURE'
and current_status = 'ACTIVE'
Group by asset_id,  facility_name,
to_char(f.from_date_time, 'HH24:MI'),to_char(f.thru_date_time, 'HH24:MI'),relay_indication ,
f.remarks,FAC.division,created_date,cascade_assets,load_relief_break_down
]]>
	</queryString>
