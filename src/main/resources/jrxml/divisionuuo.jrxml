	<queryString>
		<![CDATA[select  0 sn,' ' subhead  , 'One unsusal occurence happened in ' ||major_section ||' , '||' due to '|| cause_of_failure ||' on '||from_date_time::date
from failures f1
where type_of_failure='UNUSUAL_OCCURENCE'
and  f1.from_date_time::date= $P{fromDate}::date --param
union
select 1 sn ,  'Railway :' subhead , zone
from failures f1 , facility fac
where type_of_failure='UNUSUAL_OCCURENCE'
and  f1.from_date_time::date=$P{fromDate} ::date --param
and fac.facility_id = f1.sub_station
union
select 2 sn    ,  'Division :' subhead , division
from failures f1 , facility fac
where type_of_failure='UNUSUAL_OCCURENCE'
and  f1.from_date_time::date= $P{fromDate}::date --param
and fac.facility_id = f1.sub_station
union
select 3 sn  ,  'Major section :' subhead , f1.major_section
from failures f1 , facility fac
where type_of_failure='UNUSUAL_OCCURENCE'
and  f1.from_date_time::date=$P{fromDate}::date --param
and fac.facility_id = f1.sub_station
union
select 4 sn  ,  'Minor section :' subhead , f1.minor_section
from failures f1 , facility fac
where type_of_failure='UNUSUAL_OCCURENCE'
and  f1.from_date_time::date=$P{fromDate}::date --param
and fac.facility_id = f1.sub_station
union
select 5 sn  ,  'Date of incident :' subhead , to_char(from_date_time, 'DD-MM-yyyy')
from failures f1 , facility fac
where type_of_failure='UNUSUAL_OCCURENCE'
and  f1.from_date_time::date=$P{fromDate}::date --param
and fac.facility_id = f1.sub_station
union
select  5+sequence_id::integer sn , 	upper(failure_activity)|| ' :', special_remarks
-- --status_type_id , status_code,sequence_id , fac.zone, fac.division, f1.major_section, f1.minor_section,
--f1.sub_station,fac.facility_id, fac.facility_name, f1.location
	from 	 failure_actions_causes_impact faci,  failures f1 , status_item s, facility fac
	where type_of_failure='UNUSUAL_OCCURENCE'
	and  f1.from_date_time::date=$P{fromDate}::date --param
	and f1.location =$P{location}
	and faci.failure_seq_id::bigint = f1.id
	and status_type_id in ( 'UUO_ACTIVITY')
	and Upper(faci.failure_activity) = status_code
	and fac.facility_id = f1.sub_station
	order by sn --sequence_id::integer]]>
	</queryString>
