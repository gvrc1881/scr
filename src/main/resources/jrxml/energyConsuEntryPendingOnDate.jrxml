	<queryString>
		<![CDATA[select date_of_energy_data_pending , data_div ,  feeder_name
from
(select $P{date}::date date_of_energy_data_pending , data_div ,  feeder_name  from tss_feeder_master
where case when  $P{division}!='' then data_div = $P{division}
else data_div is not null end
and case when $P{depot}!='' then feeder_name=$P{depot}
else feeder_name is not null end
)a
except
(select $P{date}::date date_of_energy_data_facilitated ,  data_div  , location from energy_consumption
where energy_reading_date =$P{date}::date
and case when $P{division}!='' then upper(data_div) = $P{division}
else data_div is not null end
and case when $P{depot}!='' then location=$P{depot}
else location is not null end
)
order by 2]]>
	</queryString>
	<field name="date_of_energy_data_pending" class="java.sql.Date"/>
	<field name="data_div" class="java.lang.String"/>
	<field name="feeder_name" class="java.lang.String"/>
