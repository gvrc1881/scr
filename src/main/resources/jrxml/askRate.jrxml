<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="fdn_activity_wise" pageWidth="1500" pageHeight="842" whenNoDataType="NoDataSection" columnWidth="1460" leftMargin="20" rightMargin="20" topMargin="20" bottomMargin="20" whenResourceMissingType="Empty" uuid="731730b4-663b-46b7-8ddc-7a8b31d30eaa">
	<property name="ireport.zoom" value="1.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="workName" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="group" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="section" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="agency" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select
a.work_name pr, a.work_group gr, a.section section  , a.agency agency,
case when mt.rkm is null then 0 else mt.rkm end as crs_tgt_RKM ,
'' QTR,
a.population TQ ,
(a.population -
 case when p_t.total_done_till_last_month is null then 0 else p_t.total_done_till_last_month end
) as BLSOM ,
(	a.population -
	(case when p_t.total_done_till_last_month is null then 0 else p_t.total_done_till_last_month end
	 +
	 case when total_done_in_current_month is null then 0 else total_done_in_current_month end
	)
) as bal,
(	case when p_t.total_done_till_last_month is null then 0 else p_t.total_done_till_last_month end
	+
	case when total_done_in_current_month is null then 0 else total_done_in_current_month end
) as cum_done ,
case when total_done_in_current_month is null then 0 else total_done_in_current_month end as total_done_in_current_month ,
case when mt.target_date is null then 'no target date ' else to_char(mt.target_date,'dd-mm-yyyy') end as crs_target_date ,
total_done_in_current_month Total, round(total_done_in_current_month/ EXTRACT(DAY FROM now())) avg_of_month,
case when mt.target_date is null then '10000' else (mt.target_date - now()::date) end as  days_left,
((	a.population -
	(case when p_t.total_done_till_last_month is null then 0 else p_t.total_done_till_last_month end
	 +
	 case when total_done_in_current_month is null then 0 else total_done_in_current_month end
	)
)/(case when mt.target_date is null then '10000' else (mt.target_date - now()::date) end)) as Asking_rate_per_Day,

(abs((	a.population -
	(case when p_t.total_done_till_last_month is null then 0 else p_t.total_done_till_last_month end
	 +
	 case when total_done_in_current_month is null then 0 else total_done_in_current_month end
	)
)/(case when mt.target_date is null then '10000' else (mt.target_date - now()::date) end)) -
 round(total_done_in_current_month/ EXTRACT(DAY FROM now()))) as Difference_between_Asking_Rate_and_actual
 ,
 a.phase_name , a.wpa_name , a.population TQ , a.uom ,
case when mt.tkm is null then 0 else mt.tkm end as crs_tgt_TKM ,

--

case when OD_cnt.performed_count is null then 0 else OD_cnt.performed_count end as day_progress_count ,
case when p_t.total_done_till_last_month is null then 0 else p_t.total_done_till_last_month end as total_done_till_last_month
--,


--
--
/*
D1, D2, D3, D4, D5, D6, D7, D8, D9, D10, D11, D12, D13, D14, D15, D16, D17, D18, D19, D20,
 D21, D22, D23, D24, D25, D26, D27, D28, D29, D30, D31,
a.w_id , a.wp_id , a.wg_id , a.wpa_id
*/
from
	(  -- generation of work group phase activities and phases without activities for all days of a month
		select * from
		/*( -- generation of all month days
		 SELECT generate_series(date_trunc('month', now() ) ,
		 date_trunc('month', now() ) + interval '1 month' - interval '1 day'
		  , interval '1 day ')::date
		  as date_series ) ds ,*/
		( -- get population of all work group phase activities and phases without activities
		   -- filter can be applied based on status of work , work name (id), phases (based on start date and end date etc)
			select population::integer , wpa.uom ,
			wpa.work_name , wpa.work_group , wpa.section,  wpa.agency , wpa.phase_name, wpa.wpa_name, -- population ,
			'CRS_TRG' , 'QTR' ,
			--wpa.w_id, wpa.wp_id, wpa.wg_id , wpa.wpa_id --,
			wpasp.w_id, wpasp.wp_id, wpasp.wg_id , wpasp.wpa_id
			from v_work_group_phase_activities wpa
			left outer join v_work_group_phase_activity_population wpasp
			on (wpa.wpa_id = wpasp.wpa_id )
		) p
	)a
left outer join
	(
	 -- get the performance at the start of current month
	 -- for each work , section , agency , phase, actvitiy
	  select sum(performed_count) total_done_till_last_month ,
		work_name , work_group , uom , section , agency , phase_name , wpa_name , wp_sequence , wpa_sequence ,
	wpadp_work_group_id , work_group_id , work_phase_activity_id
	-- select date_trunc('month', now() )::date *
	from v_wpa_daily_progress
	where date < date_trunc('month', now() )::date
	group by  work_name , work_group , uom , section , agency , phase_name , wpa_name , wp_sequence ,
	wpa_sequence , wpadp_work_group_id , work_group_id , work_phase_activity_id
	) p_t
	on (a.wg_id = p_t.work_group_id and a.wpa_id = p_t.work_phase_activity_id  )
--  on date progress
left outer join v_wpa_daily_progress OD_cnt
    on (a.wg_id = p_t.work_group_id and a.wpa_id = p_t.work_phase_activity_id
		and date = now()::date  )
-- CRS target date and target RKM , TKM
left outer join v_milestone_targets  mt
	on (a.wg_id = mt.work_group_id and target_type = 'CRS_INSPECTION_TARGET')
---

left outer join
	(
	-- get the done count of current month
	-- for each work , section , agency , phase, activity
	select sum(performed_count) total_done_in_current_month ,
	work_name , work_group , uom , section , agency , phase_name , wpa_name , wp_sequence , wpa_sequence ,
	wpadp_work_group_id , work_group_id , work_phase_activity_id
	from v_wpa_daily_progress
	where date >=  date_trunc('month', now() )::date
	and date <= (date_trunc('month', now() ) + interval '1 month' - interval '1 day' )::date
	group by  work_name , work_group , uom , section , agency , phase_name , wpa_name , wp_sequence ,
	wpa_sequence , wpadp_work_group_id , work_group_id , work_phase_activity_id
	 ) pcm
	on (a.wg_id = pcm.work_group_id and a.wpa_id = pcm.work_phase_activity_id  )
left outer join
	(
	SELECT  split_part(dp_array, ',',1 ) as D1, split_part(dp_array, ',',2 ) as D2
	, split_part(dp_array, ',',3 ) as D3, split_part(dp_array, ',',4 ) as D4
	, split_part(dp_array, ',',5 ) as D5, split_part(dp_array, ',',6 ) as D6
	, split_part(dp_array, ',',7 ) as D7, split_part(dp_array, ',',8 ) as D8
	, split_part(dp_array, ',',9 ) as D9, split_part(dp_array, ',',10 ) as D10
	, split_part(dp_array, ',',11 ) as D11, split_part(dp_array, ',',12 ) as D12
	, split_part(dp_array, ',',13 ) as D13, split_part(dp_array, ',',14 ) as D14
	, split_part(dp_array, ',',15 ) as D15, split_part(dp_array, ',',16 ) as D16
	, split_part(dp_array, ',',17 ) as D17, split_part(dp_array, ',',18 ) as D18
	, split_part(dp_array, ',',19 ) as D19, split_part(dp_array, ',',20 ) as D20
	, split_part(dp_array, ',',21 ) as D21, split_part(dp_array, ',',22 ) as D22
	, split_part(dp_array, ',',23 ) as D23, split_part(dp_array, ',',24 ) as D24
	, split_part(dp_array, ',',25 ) as D25, split_part(dp_array, ',',26 ) as D26
	, split_part(dp_array, ',',27 ) as D27, split_part(dp_array, ',',28 ) as D28
	, split_part(dp_array, ',',29 ) as D29, split_part(dp_array, ',',30 ) as D30
	, split_part(dp_array, ',',31 ) as D31
	, work_name ,work_group , section , agency , phase_name ,wpa_name ,  population ,  uom ,
	-- case when performed_count is null then 0 else performed_count end as day_progress_count ,
	 w_id ,  wp_id , wg_id , wpa_id
	FROM
		(
		select string_agg(day_progress_count::varchar   ,',') dp_array,
		 work_name ,work_group , section , agency , phase_name ,wpa_name ,  population ,  uom ,
		-- case when performed_count is null then 0 else performed_count end as day_progress_count ,
		 w_id ,  wp_id , wg_id , wpa_id
		from
		(
		select
		-- case when p_t.total_done_till_last_month is null then 0 else p_t.total_done_till_last_month end as total_done_till_last_month ,
		-- case when total_done_in_current_month is null then 0 else total_done_in_current_month end as total_done_in_current_month ,
		date_series , a.work_name , a.work_group , a.section , a.agency , a.phase_name , a.wpa_name --,
		 , a.population , a.uom ,
		 case when performed_count is null then 0 else performed_count end as day_progress_count ,
		 w_id , wp_id , wg_id , wpa_id
		 from
			(  -- generation of work group phase activities and phases without activities for all days of a month
				select * from
					( -- generation of all month days
					 SELECT generate_series(date_trunc('month', now() ) ,
					 date_trunc('month', now() ) + interval '1 month' - interval '1 day'
					  , interval '1 day ')::date
					  as date_series ) ds ,
					--
					( -- get population of all work group phase activities and phases without activities
					   -- filter can be applied based on status of work , work name (id), phases (based on start date and end date etc)
						select population , wpa.uom ,
						wpa.work_name , wpa.work_group , wpa.section,  wpa.agency , wpa.phase_name, wpa.wpa_name, -- population ,
						'CRS_TRG' , 'QTR' ,
						--wpa.w_id, wpa.wp_id, wpa.wg_id , wpa.wpa_id --,
						wpasp.w_id, wpasp.wp_id, wpasp.wg_id , wpasp.wpa_id
						from v_work_group_phase_activities wpa
						left outer join v_work_group_phase_activity_population wpasp
						on (wpa.wpa_id = wpasp.wpa_id )
					) p
					--
			)a
		left outer join
			-- get daily done count for each work_name or w_id , section , agency , work_phase and activity
			v_wpa_daily_progress wdp
			on (a.wg_id = work_group_id and a.wpa_id = work_phase_activity_id  and date_series = wdp.date)
		) b
		--where
		--	b.wpa_name = 'Foundations'  and w_id=1 and wg_id=1
		group by
		 work_name ,work_group , section , agency , phase_name ,wpa_name --,
		 , population ,  uom ,
		 --case when performed_count is null then 0 else performed_count end as day_progress_count ,
		 w_id ,  wp_id , wg_id , wpa_id
		) r_to_c
	--
	) dp_r
on  (a.wg_id = dp_r.wg_id and a.wpa_id = dp_r.wpa_id  )

 where
case when $P{workName} != '' then a.work_name =$P{workName}
else a.work_name is not null or a.work_name is null
end

and case when $P{group} != '' then a.work_group =$P{group}
else a.work_group is not null or a.work_group is null
end

and case when $P{section} != '' then a.section =$P{section}
else a.section is not null or a.section is null
end

and case when $P{agency} != '' then a.agency =$P{agency}
else a.agency is not null or a.agency is null
end

order by  a.w_id ,  a.wp_id , a.wg_id , a.wpa_id]]>
	</queryString>
